{% extends 'base_back.html.twig' %}

 {% block title %}RightJob{% endblock %}



{% block body %}
<!-- Body Start -->
<!-- Body Start -->
<div class="wrapper _bg4586">
    <div class="_216b01">
        <div class="container">
            <div class="row justify-content-md-center">
                <div class="col-md-10">
                    <div class="section3125 rpt145">
                        {% if type=='Freelancer' %}
                        <div class="row">
                            <div class="col-lg-7">

                                <div class="dp_dt150">
                                    <div class="img148">
                                        <img src="{{ asset('picture/' ~ profile.PhotoDeProfile) }}" alt="">
