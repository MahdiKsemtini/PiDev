                                        <span style="color: #1da1f2">{{ profile.email }}</span>
                                    </div>
                                </div>
                                <ul class="_ttl120">
                                    <li>
                                        <div class="_ttl121">
                                            <div class="_ttl122">Adresse</div>
                                            <div class="_ttl123">{{ profile.adresse }}</div>
                                        </div>
                                    </li>
                                    <li>
                                        <div class="_ttl121">
                                            <div class="_ttl122">Competences</div>
                                            <div class="_ttl123">{{ profile.competences }}</div>
                                        </div>
                                    </li>
                                    <li>
                                        <div class="_ttl121">
                                            <div class="_ttl122">Langues</div>
                                            <div class="_ttl123">{{ profile.langues }}</div>
                                        </div>
                                    </li>
                                    <li>
                                        <div class="_ttl121">
                                            <div class="_ttl122">Sexe</div>
                                            <div class="_ttl123">{{ profile.sexe }}</div>
                                        </div>
                                    </li>
                                </ul>
                            </div>
                            <div class="col-lg-5">
                                <a href="#" class="_216b12">
                                    Comptes Resaux Sociaux :
                                </a>
                                <div class="rgt-145">
                                    <ul class="tutor_social_links">
                                        {% if profile.CompteFacebook =="Add Compte Facebook" %}
                                            <li><a href="#" class="fb"><i class="fab fa-facebook"></i></a></li>
                                        {% else %}
                                            <li><a href="{{ profile.CompteFacebook }}" target="_blank" class="fb"><i class="fab fa-facebook-f"></i></a></li>
                                        {% endif %}
                                        {% if profile.CompteLinkedin =="Add Compte Twitter" %}
                                            <li><a href="#" class="tw"><i class="fab fa-twitter"></i></a></li>
                                        {% else %}
                                            <li><a href="{{ profile.CompteLinkedin }}" target="_blank" class="tw"><i class="fab fa-twitter"></i></a></li>
                                        {% endif %}
                                        {% if profile.CompteTwitter =="Add Compte LinkedIn" %}
                                            <li><a href="#" class="ln"><i class="fab fa-linkedin-in"></i></a></li>
                                        {% else %}
                                            <li><a href="{{ profile.CompteTwitter }}" target="_blank" class="ln"><i class="fab fa-linkedin-in"></i></a></li>
                                        {% endif %}
                                    </ul>
                                    <br>
                                    <div><span style="color: white">Rating de :</span></div>
