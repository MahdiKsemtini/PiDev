                                            --><a href="{{ path('ratingFreelancer',{'numb':2 , 'idTaker':profile.id, 'id':profile.id}) }}" title="Give 2 stars">★</a><!--
                                            --><a href="{{ path('ratingFreelancer',{'numb':1 , 'idTaker':profile.id, 'id':profile.id}) }}" title="Give 1 star">★</a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        {% else %}
                        <div class="row">
                            <div class="col-lg-7">

                                <div class="dp_dt150">
                                    <div class="img148">
                                        <img src="{{ asset('picture/' ~ profile.PhotoDeProfile) }}" alt="">
