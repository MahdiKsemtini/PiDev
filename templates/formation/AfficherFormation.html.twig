                idv=0;
                idv=this.dataset.id;

                document.querySelector('.bg-model').style.display='flex';
            });
        }

        document.getElementById("BtnS").addEventListener('click',function () {
            var type="";
            var typec="";
            var url="";
            var num="+216"+document.getElementById("num").value;
            if(document.getElementById("call").checked){
                type="call";
            }
            if(document.getElementById("sms").checked){
                typec="SMS";
            }
             url=Routing.generate('addParticiper',{'idfo':idv,'typee': 'formation', 'num': num, 'type': type, 'typeC': typec})
            setTimeout(function(){document.location.href = url},500);
                              });


    </script>
    <script>
        document.querySelector('.close').addEventListener('click',function (){
           document.querySelector('.bg-model').style.display="none";
        });
        document.querySelector('#BtnA').addEventListener('click',function (){
            document.querySelector('.bg-model').style.display="none";
        });

    </script>


